		<videourl5>http://www.adobe.com/go/dw11morevideos_br</videourl5>
		
		<!-- video link strings -->
		<topvideos0>Novidades no Dreamweaver</topvideos0>
		<topvideos1>Criação de estilos visuais com CSS Designer</topvideos1>
